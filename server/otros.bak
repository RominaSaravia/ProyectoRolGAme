
/**
 * @param {function} cbResult 
 */
const getAll = (cbResult) => {
  fs.readFile(path.join(__dirname, "adventures.json"), (err, data) => {
    if (err) {

      cbResult([]);
    }else {
      const adventureList = JSON.parse(data)
      
      cbResult(adventureList.sort( (a, b) => { 
        if (a.name < b.name) {
          return -1;
        }
        if (a.name === b.name && a.name < b.name) {
          return -1;
        }

        return 1;
           
        
      }));
    }

})
}